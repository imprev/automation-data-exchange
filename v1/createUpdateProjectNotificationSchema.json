{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "event": {
      "type": "string",
      "enum": ["createProjectNotification","updateProjectNotification"]
    },
    "eventMessageVersion": {
      "type": "integer"
    },
    "eventOccurredTimestamp": {
      "type": "integer"
    },
    "id": {
      "type": "string"
    },
    "externalUserId": {
      "type": "string"
    },
    "imprevUserId": {
      "type": "integer"
    },
    "imprevAppId": {
      "type": "integer"
    },
    "imprevListingId": {
      "type": "string"
    },
    "externalListingId": {
      "type": "string"
    },
    "mlsListingId": {
      "type": "string"
    },
    "projectId": {
      "type": "integer"
    },
    "printableDesignName": {
      "type": "string"
    },
    "designType": {
      "type": "string"
    },
    "listingEvent": {
      "type": "string"
    },
    "editProjectUrl": {
      "type": "string"
    },
    "distributions": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "distributionId": {
              "type": "integer"
            },
            "url": {
              "type": "string"
            },
            "aliasedUrl": {
              "type": "string"
            },
            "shortUrl": {
              "type": "string"
            },
            "mimeType": {
              "type": "string"
            },
            "isIDX": {
              "type": "boolean"
            }
          },
          "required": [
            "distributionId",
            "url",
            "isIDX"
          ]
        }
      ]
    }
  },
  "required": [
    "event",
    "eventMessageVersion",
    "eventOccurredTimestamp",
    "id",
    "externalUserId",
    "imprevUserId",
    "imprevAppId",
    "imprevListingId",
    "externalListingId",
    "projectId",
    "printableDesignName",
    "designType",
    "listingEvent"
  ]
}
